    "lint": "eslint"
  },
  "author": "Fox Islam",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "prettier": "^2.2.1",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "@types/node": "^14.14.34",
    "tone": "^14.7.77"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fox-Islam/krdict.js.git"
  },
  "keywords": [
